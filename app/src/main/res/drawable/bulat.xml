<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Latar belakang putih -->
    <item>
        <shape
            android:shape="oval">
            <solid android:color="#000000" /> <!-- Putih -->
        </shape>
    </item>
    <!-- Pinggiran hitam -->
    <item
        android:left="4dp"
        android:top="4dp"
        android:right="4dp"
        android:bottom="4dp">
        <shape
            android:shape="oval">
            <solid android:color="#FFFFFF" /> <!-- Hitam -->
        </shape>
    </item>
</layer-list>
